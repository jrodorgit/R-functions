jro_sd <- function(a) {

	m <- mean(a)
	res <- sqrt(sum( (a-m)**2)/length(a))
	return( res )

}

jro_se <- function(population,sampleSize) {
	sd <- jro_sd(population)
	return ( sd / sqrt(sampleSize) )
}
